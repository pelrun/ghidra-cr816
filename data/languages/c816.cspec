<?xml version="1.0" encoding="UTF-8"?>

<compiler_spec>
  <global>
    <range space="code"/>
    <range space="data"/>
  </global>
  <readonly>
    <range space="code" />
  </readonly>
  <stackpointer register="I3" space="data" growth="negative"/>
  <default_proto>
    <prototype name="default" extrapop="0" stackshift="0" strategy="register">
      <returnaddress>
        <register name="IP" />
      </returnaddress>
      <input>
        <pentry minsize="1" maxsize="1">
          <register name="R3"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R2"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R1"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R0"/>
        </pentry>
      </input>
      <output>
        <pentry minsize="1" maxsize="1">
          <register name="R3"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R2"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R1"/>
        </pentry>
        <pentry minsize="1" maxsize="1">
          <register name="R0"/>
        </pentry>
      </output>
    </prototype>
  </default_proto>
</compiler_spec>
